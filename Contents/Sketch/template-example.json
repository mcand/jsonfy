
/* Suporte e validação das tags + atributos */

{
  "tags": {
    "a": {
      "attributes": {
        "attribute1": {
          "background-color": "/^{0-9a-f}{2,6}$/"
        }
      }
    },
    "h1": {
      "attributes": {
        "attribute1": {
          "validation": "regex"
        }
      }
    }
  }
}

